{
	"name": "Chrome Crypto",
	"version": "1.0",
	"default_locale": "en",
	"manifest_version": 2,
	"description": "Replace your chrome new tab page with a list of popular cryptocurrencies and their current prices.",
	"homepage_url": "https://github.com/omeiza/chrome-crypto",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"chrome_url_overrides": {
		"newtab": "page-override/index.html"
	}
}